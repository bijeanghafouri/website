if(is.na(first) == T){first <- 0}
if((first == 'NaN') == TRUE){first <- 0}
# second term
second <-  total_probability[2]*(-prop_2[1]*log(prop_2[1]) - prop_2[2]*log(prop_2[2]))
if(is.na(second) == T){second <- 0}
if(second == 'NaN'){second <- 0}
# entropy
entropy <- first + second
}
else if(nrow(t) == 3){
prop_1 <- t[1, ]/sum(t[1, ])
